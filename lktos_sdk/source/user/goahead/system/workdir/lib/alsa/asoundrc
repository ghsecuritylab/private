pcm.mv_audio {
	type hw
	card 0
	device 0
}

pcm_slave.mv_rate {
	pcm mv_audio
	rate 44100
}

pcm.rate_convert {
	type rate
	slave mv_rate
}



pcm.!default {
	type plug
	slave.pcm "dmixer"
}

pcm.dmixer {
	type dmix
	ipc_key 1024
	slave {
		pcm "hw:0,0"
		period_time 0
		period_size 1024
		buffer_size 4096
		rate 44100
	}
	
	bindings {
		0 0
		1 1
	}
}

ctl.dmixer {
	type hw
	card 0
}

